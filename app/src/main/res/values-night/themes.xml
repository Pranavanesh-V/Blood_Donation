<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.BloodDonation" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="colorPrimary">@color/g</item>
        <item name="android:statusBarColor" >?attr/colorPrimary</item>
        <item name="android:background" >?attr/background</item>
    </style>

    <style name="page8.Blood_Donation" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="hintTextColor">@color/white</item>
        <item name="boxBackgroundColor">@color/box_col</item>
    </style>

    <style name="bg" parent="page8.Blood_Donation">
        <item name="tint">@color/white</item>
    </style>

    <style name="bg1" parent="page8.Blood_Donation">
        <item name="android:background">@color/box_col</item>
    </style>

    <style name="button_attr" parent="page8.Blood_Donation">
        <item name="android:backgroundTint">@color/box_col</item>
    </style>

    <style name="radio_attr" parent="page8.Blood_Donation">
        <item name="buttonTint">@color/white_bg</item>
        <item name="android:textColor">@color/white_bg</item>
    </style>

    <style name="page71.Blood_Donation" parent="Theme.MaterialComponents.NoActionBar">

        <item name="colorPrimary">@color/grey_black</item>
        <item name="hintTextColor">@color/white</item>
        <item name="colorOutline">@color/box_col</item>
        <item name="backgroundColor">@color/grey_black</item>
    </style>
    <style name="page72.Blood_Donation" parent="Theme.MaterialComponents.NoActionBar">

        <item name="colorPrimary">@color/g</item>
        <item name="hintTextColor">@color/white</item>
        <item name="colorOutline">@color/box_col</item>
    </style>
    <style name="MyTextInputLayout_FloatingLabel" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <!-- Set the color of the floating label text -->
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50dp</item>
    </style>


</resources>