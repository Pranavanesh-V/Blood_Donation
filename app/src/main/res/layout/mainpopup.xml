<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/popupbg"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@id/bottom"
        app:layout_constraintEnd_toEndOf="@id/end"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/start"
        app:layout_constraintTop_toTopOf="@id/top"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="UselessParent">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:importantForAccessibility="no"
            android:src="@drawable/device"
            app:layout_constraintBottom_toTopOf="@id/txt1_bottom"
            app:layout_constraintEnd_toStartOf="@id/img_end"
            app:layout_constraintStart_toEndOf="@id/img_start"
            app:layout_constraintTop_toBottomOf="@id/txt1_top"
            tools:ignore="ImageContrastCheck" />

        <Button
            android:id="@+id/from_device"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/popbtn"
            android:fontFamily="@font/poppins"
            android:gravity="center"
            android:text="@string/choose_from_device"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@id/txt1_bottom"
            app:layout_constraintEnd_toStartOf="@id/txt_end"
            app:layout_constraintStart_toEndOf="@id/txt_start"
            app:layout_constraintTop_toBottomOf="@id/txt1_top"
            tools:ignore="VisualLintButtonSize" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:importantForAccessibility="no"
            android:src="@drawable/delete"
            app:layout_constraintBottom_toTopOf="@id/txt2_bottom"
            app:layout_constraintEnd_toStartOf="@id/img_end"
            app:layout_constraintStart_toEndOf="@id/img_start"
            app:layout_constraintTop_toBottomOf="@id/txt2_top"
            tools:ignore="ImageContrastCheck" />

        <Button
            android:id="@+id/Delete_pro"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/popbtn"
            android:fontFamily="@font/poppins"
            android:gravity="center"
            android:text="@string/delete_profile"
            android:textColor="@color/black"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@id/txt2_bottom"
            app:layout_constraintEnd_toStartOf="@id/txt_end"
            app:layout_constraintStart_toEndOf="@id/txt_start"
            app:layout_constraintTop_toBottomOf="@id/txt2_top"
            tools:ignore="VisualLintButtonSize" />

        <Button
            android:id="@+id/cancel_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|right"
            android:background="@drawable/cancel_pop"
            android:text="@string/cancel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/txt_end"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@id/txt_start"
            app:layout_constraintTop_toBottomOf="@id/txt2_bottom"
            app:layout_constraintVertical_bias="0.5"
            tools:ignore="RtlHardcoded,TextContrastCheck" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/img_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent=".07" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/img_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.23" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.27" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent=".93" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt1_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt1_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent=".35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt2_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent=".38" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/txt2_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent=".68" />


    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".04" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".63" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".97" />




</androidx.constraintlayout.widget.ConstraintLayout>